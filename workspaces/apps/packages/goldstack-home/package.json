{
  "name": "@goldstack/goldstack-home",
  "version": "0.1.0",
  "private": true,
  "author": "Max Rohde",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:next": "next build && next export -o webDist/",
    "clean": "rimraf ./dist ./webDist",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc --isolatedModules false -p tsconfig.compile.json --watch",
    "compile:sass": "node-sass src/styles/app.scss src/styles/app.css --source-map src/styles/app.css.map",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "deploy": "template-nextjs set-nextjs-env $@ && yarn build && yarn web:build && template deploy $@",
    "deploy-ts": "yarn template-ts set-nextjs-env $@ && yarn build && yarn web:build && yarn template-ts deploy $@",
    "generate-docs": "yarn generate-docs:project && yarn generate-docs:awsUser",
    "generate-docs:awsUser": "ts-node --project tsconfig.local.json ./scripts/generateDocs.ts generate-doc --json goldstack/configuration/aws-user-project-config.md > ./src/docs/aws-user.json",
    "generate-docs:project": "ts-node --project tsconfig.local.json ./scripts/generateDocs.ts generate-doc --json goldstack/configuration/project.md > ./src/docs/project.json",
    "generate-schema": "ts-node --project tsconfig.local.json src/lib/types/generateSchemas.ts",
    "infra": "template-nextjs infra",
    "next:tracking:disable": "next telemetry disable",
    "next:watch": "next",
    "prepublishOnly": "yarn run build",
    "start:next": "next start",
    "template": "template-nextjs",
    "template-ts": "ts-node --project tsconfig.local.json scripts/template.ts",
    "test": "yarn test-ci --watch",
    "test-ci": "jest --passWithNoTests --config=./jest.config.js",
    "watch": "yarn next:watch",
    "web:build": "NODE_ENV=production yarn build:next",
    "web:watch": "yarn web:build && concurrently \"reload --dir ./webDist --watch-dir ./webDist --port 8071 --browser\" "
  },
  "dependencies": {
    "@fullstory/browser": "^1.4.5",
    "@goldstack/goldstack-api": "0.1.0",
    "@goldstack/infra": "0.3.30",
    "@goldstack/infra-aws": "0.3.38",
    "@goldstack/project-config": "0.1.0",
    "@goldstack/project-repository": "0.1.0",
    "@goldstack/template-metadata": "0.1.0",
    "@goldstack/utils-package": "0.3.32",
    "@goldstack/utils-project": "0.3.28",
    "@goldstack/utils-track": "0.1.13",
    "@rjsf/bootstrap-4": "^2.4.0",
    "@rjsf/core": "^2.4.0",
    "@stripe/stripe-js": "^1.10.0",
    "ajv": "6.12.2",
    "data-uri-to-buffer": "^3.0.1",
    "file-loader": "^6.2.0",
    "img-loader": "^4.0.0",
    "install": "^0.13.0",
    "next": "^11.0.1",
    "next-optimized-images": "^2.6.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-scrollable-anchor": "^0.6.1",
    "styled-components": "^5.0.0",
    "swr": "^0.4.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@goldstack/template-nextjs": "0.3.100",
    "@goldstack/utils-docs-cli": "0.3.11",
    "@goldstack/utils-sh": "0.4.26",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@types/react": "^16.9.46",
    "@types/react-scrollable-anchor": "^0.6.0",
    "@types/styled-components": "^5.1.1",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@types/testing-library__react": "^10.0.1",
    "babel-plugin-styled-components": "^1.8.0",
    "concurrently": "^5.2.0",
    "extracted-loader": "^1.0.7",
    "ignore-loader": "^0.1.2",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^8.0.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-json-schema-generator": "^0.96.0",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3"
  }
}
