#!/usr/bin/env node

// eslint-disable-next-line @typescript-eslint/no-var-requires
require('source-map-support').install();

// eslint-disable-next-line @typescript-eslint/no-var-requires
require('./../dist/src/templateEmailSendCli')
  .run(process.argv)
  .catch((e) => {
    console.log(e);
    process.exit(1);
  });
