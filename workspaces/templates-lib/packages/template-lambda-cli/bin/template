#!/usr/bin/env node
/* eslint-disable @typescript-eslint/no-var-requires */

// eslint-disable-next-line @typescript-eslint/no-var-requires
require('source-map-support').install();

require('./../dist/src/templateLambdaExpress')
  .run(process.argv)
  .catch((e) => {
    console.log(e);
    process.exit(1);
  });
