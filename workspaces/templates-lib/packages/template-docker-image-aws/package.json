{
  "name": "@goldstack/template-docker-image-aws",
  "version": "0.3.81",
  "description": "Utilities for packages that are deployed as Docker Images using AWS Fargate",
  "keywords": [
    "goldstack",
    "utility",
    "infrastructure",
    "IaC",
    "fargate",
    "ecs",
    "aws",
    "docker",
    "configuration"
  ],
  "homepage": "https://goldstack.party",
  "bugs": {
    "url": "https://github.com/goldstack/goldstack/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goldstack/goldstack.git"
  },
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/templateDockerImageAws.ts",
  "bin": {
    "template": "./bin/template",
    "template-docker-image-aws": "./bin/template"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:watch": "yarn clean && yarn compile-watch",
    "clean": "rimraf ./dist",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "compile-watch:light": "nodemon --watch ./src/ -e '*' --exec 'yarn compile'",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "generate-schema": "ts-node scripts/generateSchemas.ts AWSDockerImage && cp schemas/* ../../../templates/packages/docker-image-aws/schemas",
    "prepublishOnly": "yarn run build",
    "publish": "utils-git changed --exec \"yarn npm publish $@\"",
    "test-ci": "jest --passWithNoTests --config=./jest.config.js --runInBand",
    "version:apply": "utils-git changed --exec \"yarn version $@ && yarn version apply\"",
    "version:apply:force": "yarn version $@ && yarn version apply"
  },
  "dependencies": {
    "@goldstack/infra": "0.3.36",
    "@goldstack/infra-aws": "0.3.44",
    "@goldstack/utils-aws-cli": "0.3.54",
    "@goldstack/utils-cli": "0.2.13",
    "@goldstack/utils-config": "0.3.33",
    "@goldstack/utils-docker": "0.3.38",
    "@goldstack/utils-git": "0.1.34",
    "@goldstack/utils-log": "0.2.14",
    "@goldstack/utils-package": "0.3.38",
    "@goldstack/utils-package-config": "0.3.39",
    "@goldstack/utils-s3-deployment": "0.3.55",
    "@goldstack/utils-sh": "0.4.31",
    "@goldstack/utils-template": "0.3.35",
    "@goldstack/utils-terraform": "0.3.68",
    "@goldstack/utils-terraform-aws": "0.3.67",
    "aws-sdk": "2.1055.0",
    "source-map-support": "^0.5.21",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@goldstack/utils-git": "0.1.34",
    "@goldstack/utils-package-config-generate": "0.2.14",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "@types/source-map-support": "^0.5.4",
    "@types/yargs": "^15.0.5",
    "jest": "^28.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "publishConfig": {
    "main": "dist/src/templateDockerImageAws.ts"
  }
}
