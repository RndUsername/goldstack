{
  "name": "@goldstack/docker-image-aws",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/image.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile && yarn docker:build $@",
    "clean": "yarn docker:clean",
    "compile": "tsc -p tsconfig.json",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "deploy": "yarn docker:build $@ && template deploy $@",
    "docker:build": "ts-node ./src/build.ts build $@",
    "docker:clean": "ts-node ./src/build.ts clean $@",
    "image": "template image",
    "infra": "template infra",
    "prepublishOnly": "yarn run build",
    "template": "template",
    "template-ts": "ts-node scripts/template.ts",
    "test-ci": "jest --passWithNoTests --config=./jest.config.js"
  },
  "dependencies": {
    "@goldstack/template-docker-image-aws": "0.3.78",
    "@goldstack/utils-git": "0.1.32",
    "@goldstack/utils-sh": "0.4.29"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.0",
    "concurrently": "^5.2.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3"
  }
}
