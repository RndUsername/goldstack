{
  "name": "@goldstack/docs-main",
  "version": "0.1.0",
  "license": "",
  "author": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:next": "next build && rimraf ./webDist && next export -o webDist/",
    "clean": "rimraf ./dist ./webDist",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.compile.json --watch",
    "compile:sass": "node-sass src/styles/app.scss src/styles/app.css --source-map src/styles/app.css.map",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "template-nextjs set-nextjs-env $@ && yarn build && yarn web:build && template deploy $@",
    "deploy-ts": "yarn template-ts set-nextjs-env $@ && yarn build && yarn web:build && yarn template-ts deploy $@",
    "generate-docs": "ts-node --project tsconfig.local.json ./scripts/generateDocs.ts",
    "infra": "template-nextjs infra",
    "next:tracking:disable": "next telemetry disable",
    "next:watch": "next",
    "prepublishOnly": "yarn run build",
    "start:next": "next start",
    "template": "template-nextjs",
    "template-ts": "ts-node --project tsconfig.local.json scripts/template.ts",
    "test": "jest --config=./jest.config.js --watch",
    "test-ci": "jest --passWithNoTests --config=./jest.config.js --runInBand",
    "watch": "yarn next:watch -- -p 3001",
    "web:build": "yarn build:next",
    "web:watch": "yarn web:build && concurrently \"reload --dir ./webDist --watch-dir ./webDist --port 8071 --browser\" "
  },
  "dependencies": {
    "@fullstory/browser": "^1.4.5",
    "@goldstack/toc-generator": "0.1.0",
    "@goldstack/utils-track": "0.1.18",
    "cheerio": "1.0.0-rc.10",
    "file-loader": "^6.2.0",
    "ignore-loader": "^0.1.2",
    "img-loader": "^4.0.0",
    "next": "^12.2.1",
    "next-optimized-images": "^2.6.2",
    "react": "^18.2.0",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.5",
    "swr": "^0.4.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@goldstack/markdown-docs": "0.1.0",
    "@goldstack/template-nextjs": "0.3.113",
    "@goldstack/utils-sh": "0.4.33",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.15",
    "@types/styled-components": "^5.1.25",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@types/testing-library__react": "^10.0.1",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^5.2.0",
    "extracted-loader": "^1.0.7",
    "ignore-loader": "^0.1.2",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-transform-stub": "^2.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^8.0.0",
    "node-sass": "7.0.0",
    "reload": "^3.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
